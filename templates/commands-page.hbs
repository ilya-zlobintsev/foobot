<!doctype html>
<style>
  body {
    background-color: black;
  }
  h1 {
    color: white;
  }
  table {
    color: white;
  }
</style>
<section id="hello">
  <h1>Commands in {{ channel }}</h1>
  <table border=1>
    <thead>
      <th>Command</th>
      <th>Action</th>
      <th>Permissions</th>
    </thead>
    <tbody>
      {{#each commands}}
      <tr>
        <td>{{ this.trigger }}</td>
        <td>{{ this.action }}</td>
        <td>{{ this.permissions }}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</section>